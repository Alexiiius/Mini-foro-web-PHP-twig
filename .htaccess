RewriteEngine on
# RewriteRule ^expresion_regular$ url_real [flags_opcionales]


RewriteRule ^login$ index.php [QSA,L]
RewriteRule ^logout$ index.php?controlador=Usuario&funcion=cerrarSesion [L]
